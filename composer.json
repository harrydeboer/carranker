{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url" : "https://wpackagist.org"
        }
    ],
    "require": {
        "php": "7.4.*",
        "ext-bcmath": "*",
        "ext-calendar": "*",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-date": "*",
        "ext-dom": "*",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-filter": "*",
        "ext-ftp": "*",
        "ext-gd": "*",
        "ext-hash": "*",
        "ext-iconv": "*",
        "ext-igbinary": "*",
        "ext-imagick": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-mysqli": "*",
        "ext-mysqlnd": "*",
        "ext-openssl": "*",
        "ext-pcntl": "*",
        "ext-pcre": "*",
        "ext-PDO": "*",
        "ext-pdo_mysql": "*",
        "ext-Phar": "*",
        "ext-posix": "*",
        "ext-readline": "*",
        "ext-redis": "*",
        "ext-Reflection": "*",
        "ext-session": "*",
        "ext-SimpleXML": "*",
        "ext-sockets": "*",
        "ext-sodium": "*",
        "ext-SPL": "*",
        "ext-sysvmsg": "*",
        "ext-sysvsem": "*",
        "ext-sysvshm": "*",
        "ext-tokenizer": "*",
        "ext-xml": "*",
        "ext-xmlreader": "*",
        "ext-xmlwriter": "*",
        "ext-zip": "*",
        "ext-Zend-OPcache": "*",
        "ext-zlib": "*",
        "doctrine/dbal": "2.10.2",
        "fideloper/proxy": "4.3.0",
        "fruitcake/laravel-cors": "1.0.6",
        "guzzlehttp/guzzle": "6.5.3",
        "johnpbloch/wordpress": "5.4.1",
        "laravel/framework": "7.11.0",
        "laravel/passport": "8.5.0",
        "laravel/tinker": "2.4.0",
        "laravel/ui": "2.0.3",
        "laravelcollective/html": "6.1.0",
        "wpackagist-plugin/jwt-authentication-for-wp-rest-api": "1.2.6",
        "wpackagist-plugin/user-role-editor": "4.53.1",
        "wpackagist-plugin/security-ninja": "5.103"
    },
    "require-dev": {
        "facade/ignition": "2.0.4",
        "fzaninotto/faker": "1.9.1",
        "laravel/dusk": "6.1.0",
        "mockery/mockery": "1.3.1",
        "nunomaduro/collision": "4.2.0",
        "phpunit/phpunit": "8.5.4",
        "wp-phpunit/wp-phpunit": "5.4.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        },
        "wordpress-install-dir": "wordpress/wp",
        "installer-paths"      : {
            "public/wp-content/plugins/{$name}"   : ["type:wordpress-plugin"]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php wpUpgradeDbAndTranslations.php",
            "php -r \"copy('wordpress/.htaccess.basic', 'wordpress/wp/.htaccess');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
