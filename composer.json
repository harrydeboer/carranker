{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "require": {
        "php": "8.0",
        "ext-PDO": "*",
        "ext-Phar": "*",
        "ext-Reflection": "*",
        "ext-SPL": "*",
        "ext-SimpleXML": "*",
        "ext-Zend-OPcache": "*",
        "ext-bcmath": "*",
        "ext-calendar": "*",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-date": "*",
        "ext-dom": "*",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-filter": "*",
        "ext-ftp": "*",
        "ext-gd": "*",
        "ext-hash": "*",
        "ext-iconv": "*",
        "ext-igbinary": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-mysqli": "*",
        "ext-mysqlnd": "*",
        "ext-openssl": "*",
        "ext-pcntl": "*",
        "ext-pcre": "*",
        "ext-pdo_mysql": "*",
        "ext-posix": "*",
        "ext-readline": "*",
        "ext-redis": "*",
        "ext-session": "*",
        "ext-sockets": "*",
        "ext-sodium": "*",
        "ext-sysvmsg": "*",
        "ext-sysvsem": "*",
        "ext-sysvshm": "*",
        "ext-tokenizer": "*",
        "ext-xml": "*",
        "ext-xmlreader": "*",
        "ext-xmlwriter": "*",
        "ext-zip": "*",
        "ext-zlib": "*",
        "doctrine/dbal": "3.0.0",
        "elasticsearch/elasticsearch": "~7.10",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "johnpbloch/wordpress": "5.6.0",
        "laravel/framework": "^8.12",
        "laravel/passport": "10.1.0",
        "laravel/tinker": "^2.5",
        "laravel/ui": "3.1.0",
        "laravelcollective/html": "6.2.0",
        "wpackagist-plugin/jwt-auth": "1.4.2",
        "wpackagist-plugin/security-ninja": "5.111",
        "wpackagist-plugin/user-role-editor": "4.57.1"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",
        "laravel/dusk": "6.9.1",
        "wp-phpunit/wp-phpunit": "5.6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "wordpress-install-dir": "wordpress/wp",
        "installer-paths": {
            "public/wp-content/plugins/{$name}": [
                "type:wordpress-plugin"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php wpUpgradeDbAndTranslations.php"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
